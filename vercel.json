{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/swagger.json",
      "dest": "/public/swagger.json"
    },
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "/",
      "dest": "src/server.ts"
    }
  ]
}
